<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
			      https://maven.apache.org/xsd/settings-1.0.0.xsd">
  <servers>
    <server>
      <id>repo.icatproject.org</id>
      <username>$user</username>
      <password>$pass</password>
    </server>
  </servers>
  <profiles>
    <profile>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <serverUrl>https://payara:8181</serverUrl>
        <luceneUrl>https://payara:8181</luceneUrl>
        <searchEngine>LUCENE</searchEngine>
        <searchUrls>https://payara:8181</searchUrls>
        <containerHome>/opt/payara6</containerHome>
        <testHome>/opt/payara6/test</testHome>
      </properties>
    </profile>
  </profiles>
</settings>
